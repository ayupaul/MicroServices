{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "ProductAPIService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "Instance": {
      "NonSecurePort": 8080,
      "HostName": "productapi",
      "InstanceId": "productapiservice,Port:8080"

      //"HealthCheckUrlPath": "/api/values/healthcheck"
      //"StatusPageUrlPath": "/api/values/status"
    }

  }
}
