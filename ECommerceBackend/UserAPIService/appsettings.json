{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": {
      "name": "UserAPIService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "Instance": {
      "NonSecurePort": 8080,
      "HostName": "userapi",
      "InstanceId": "userapiservice,Port:8080"

      
    }

  }
}